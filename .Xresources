! DO NOT CHANGE THE ORDER

! URXVT APPEARANCE
URxvt.font:                       xft:mplus Nerd Font Mono:style:regular:size=12
URxvt.geometry:                   84x15
URxvt.internalBorder:             20
URxvt.cursorBlink:                true
URxvt.cursorUnderline:            true
URxvt.saveline:                   32768
URxvt.scrollBar:                  false
URxvt.scrollBar_right:            true
URxvt.scrollBar_floating:         false
URxvt.scrollstyle:                plain
URxvt.urgentOnBell:               false
URxvt.depth:                      24
URxvt.iso14755:                   false
!URxvt.iconFile:                   /home/violet/.icons/gladient/term.png
! Uncomment if to much font spacing
!URxvt.letterSpace: -1

! URXVT EXTENSIONS
URxvt.perl-ext-common:            default,clipboard,keyboard-select,url-select,tabbedex

URxvt.tabbedex.no-tabbedex-keys:  yes
URxvt.tabbedex.new-button:        false
URXvt.tabbedex.reopen-on-close:   yes
URxvt.tabbedex.autohide:          yes

URxvt.tabbedex.tabbar-fg:         1
URxvt.tabbedex.tabbar-bg:         0
URxvt.tabbedex.tab-fg:            4
URxvt.tabbedex.tab-bg:            0
URxvt.tabbedex.bell-fg:           0
URxvt.tabbedex.bell-bg:           0
URxvt.tabbedex.bell-tab-fg:       0
URxvt.tabbedex.bell-tab-bg:       0
URxvt.tabbedex.title-fg:          7
URxvt.tabbedex.title-bg:          0

URxvt.keysym.Control-Shift-T:     perl:tabbedex:new_tab
URxvt.keysym.Control-Shift-R:     perl:tabbedex:rename_tab
URxvt.keysym.Control-Shift-W:     perl:tabbedex:kill_tab
URxvt.keysym.Control-Shift-C:     perl:clipboard:copy
URxvt.keysym.Control-Shift-V:     perl:clipboard:paste
URxvt.keysym.Control-Next:        perl:tabbedex:next_tab
URxvt.keysym.Control-Prior:       perl:tabbedex:prev_tab
URxvt.keysym.Control-Shift-Next:  perl:tabbedex:move_tab_right
URxvt.keysym.Control-Shift-Prior: perl:tabbedex:move_tab_left

URxvt.resize-font.step: 1
URxvt.keysym.Control-minus:       resize-font:smaller
URxvt.keysym.Control-plus:        resize-font:bigger
URxvt.keysym.Control-equal:       resize-font:reset
URxvt.keysym.Control-question:    resize-font:show

URxvt.clipboard.copycmd:          xclip -i -selection clipboard
URxvt.clipboard.pastecmd:         xclip -o -selection clipboard
URxvt.keysym.M-c:                 perl:clipboard:copy
URxvt.keysym.M-v:                 perl:clipboard:paste
URxvt.keysym.M-Escape:            perl:keyboard-select:activate
URxvt.keysym.M-s:                 perl:keyboard-select:search

URxvt.urlLauncher:                chromium
URxvt.underlineURLs:              true
URxvt.urlButton:                  1

! COLOR SCHEME
! black > red > green > yellow > blue > magenta > cyan > white
#define black0                    #101010
#define black1                    #101010
#define black2                    #383838
#define black3                    #424242
#define magenta0                  #FA5AA4
!#f56276
#define magenta1                  #FA74B2
!#f58e9a
#define green0                    #63C5EA 
#define green1                    #7ddcff
#define yellow0                   #4989ff
#define yellow1                   #56A7F5
#define blue0                     #5c8699
!#396173
!#67a8c9 
#define blue1                     #6b9ab3
!#486778
!#6da2bd 
#define red0                      #f52541
#define red1                      #f54960
#define cyan0                     #89CCF7
#define cyan1                     #A1D5F7
#define white0                    #F9F9F9
#define white1                    #F7F7F7
#define white2                    #F4F4F4

*.color1:                         red0
*.color2:                         blue0
*.color3:                         yellow0
*.color4:                         green0
*.color5:                         magenta0
*.color6:                         cyan0

*.color9:                         red1
*.color10:                        blue1
*.color11:                        yellow1
*.color12:                        green1
*.color13:                        magenta1
*.color14:                        cyan1

! FOR REVERSING ONLY (BLACK-WHITE)
! --------------------------------
*.foreground:                     white0
*.background:                     black0
*.cursorColor:                    white0

*.color0:                         black2
*.color7:                         white1

!*.color8:                         black3
!*.color15:                        white2

!*.foreground:                     black0
!*.background:                     white0
!*.cursorColor:                    black0

!*.color0:                         white1
!*.color7:                         black2

!*.color8:                         white2
!*.color15:                        black3
